{
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org"
        }
    ],
    "require": {
        "johnpbloch/wordpress": "^5.2",
        "wpackagist-theme/twentynineteen":"1.3",
        "wpackagist-plugin/advanced-custom-fields":"5.8.0",
        "wpackagist-plugin/advanced-custom-fields-font-awesome":"3.0.2",
        "wpackagist-plugin/contact-form-7":"5.1.1"
    },
    "require-dev": {
        "wpackagist-plugin/what-the-file":"1.5.4",
        "wpackagist-plugin/query-monitor":"3.3.5",
        "wpackagist-plugin/simply-show-hooks":"1.2.1",
        "wpackagist-plugin/rewrite-rules-inspector":"1.2.1"
    },
    "extra": {
        "wordpress-install-dir": "wp",
        "installer-paths": {
            "content/themes/{$name}": ["type:wordpress-theme"],
            "content/plugins/{$name}": ["type:wordpress-plugin"]
        }
    }
}